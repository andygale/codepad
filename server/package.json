{
  "name": "codecrush-server",
  "version": "1.0.0",
  "description": "Backend server for CodeCrush collaborative coding platform",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "npx node-pg-migrate -m db/migrations --no-check-order up",
    "migrate:prod": "DATABASE_URL=\"${DATABASE_URL}?ssl=true\" npx node-pg-migrate -m db/migrations --no-check-order up",
    "migrate:create": "npx node-pg-migrate create --dir db/migrations --migration-file-language js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "codecrush",
    "collaboration",
    "websocket",
    "code-execution"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.12",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.1",
    "socket.io": "^4.7.5",
    "tar": "^6.2.0",
    "pyright": "^1.1.361",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
